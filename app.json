{
  "expo": {
    "name": "Fun Foot",
    "slug": "fun-foot-mobile",
    "description": "Fun Foot is a free sports betting app that allows you to win real money. Bet on your favorite football matches and share your winnings with the community. The more you bet, the more you win!",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a5684"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.agprogress.funfoot",
      "googleServicesFile": "./GoogleService-Info.plist"

    },
    "android": {
      "googleServicesFile":"./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a5684"
      },
      "package": "com.agprogress.funfoot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "66f8e4b2-825d-4bb3-8086-b615786a4cbb"
      }
    },
    "plugins": [
     [ "expo-build-properties", {
        "ios": {
          "useFrameworks": "static"
        }
      }],
      "@react-native-firebase/app"
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-6300362813805470~3620870563",
    "ios_app_id": "ca-app-pub-6300362813805470~6771777236"
  }
}
